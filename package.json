{
  "name": "eslint-plugin-jsx-classname",
  "version": "0.0.0",
  "description": "ESLint plugin that requires className attribute on HTML elements in JSX and TSX files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "spell": "cspell lint '**/*.{ts,js,md,json}' --gitignore",
    "spell:check": "cspell lint '**/*.{ts,js,md,json}' --gitignore",
    "knip": "knip",
    "release": "release-it",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tupe12334/eslint-plugin-jsx-classname.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "jsx",
    "classname",
    "className",
    "react",
    "accessibility"
  ],
  "author": "tupe12334",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.29.3",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cspell/dict-he": "^4.0.5",
    "@typescript-eslint/utils": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "cspell": "^9.6.4",
    "eslint": "^9.39.2",
    "eslint-config-agent": "^2.0.0",
    "eslint-config-publishable-package-json": "^1.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "tailwind-merge": "^3.4.1"
  }
}
